{
  "name": "@sylvesterllc/bishop",
  "version": "0.0.1",
  "description": "Bishop is a simplified User Manager for AWS Cognito",
  "main": "app/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/sylvesterllc/bishop.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@sylvesterllc"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "publish:lib": "rm -rf ./app && npm run build && npm version patch && npm publish",
    "build": "tsc"
  },
  "keywords": [],
  "author": "Davis Sylvester <davis@sylvesterllc.com>",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.216.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.216.0",
    "@sylvesterllc/utility-helper": "0.0.2",
    "aws-sdk": "^2.1261.0",
    "dotenv": "^16.0.3",
    "luxon": "^3.1.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@cucumber/cucumber": "^8.9.0",
    "@jest/globals": "^29.3.1",
    "@jest/types": "^29.3.1",
    "@types/jest": "^29.2.3",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.9",
    "ava": "^5.1.0",
    "babel-jest": "^29.3.1",
    "jasmine": "^4.5.0",
    "jest": "^29.3.1",
    "jest-config": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "ava": {
    "files": [
      "./__tests__/**/*"
    ],
    "match": [
      "*test*",
      "!foo"
    ],
    "extensions": {
      "ts": "module"
    },
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "environmentVariables": {},
    "verbose": true,
    "require": [],
    "nodeArguments": [
      "--trace-deprecation",
      "--napi-modules",
      "--loader=ts-node/esm"
    ]
  }
}
